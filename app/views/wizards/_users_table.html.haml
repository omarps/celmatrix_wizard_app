%table.table.table-striped
  %thead.thead-dark
    %tr
      %th First Name
      %th Last Name
      %th Email Address
      %th Age
      %th Height
      %th Weight
      %th Favorite Color
  %tbody.table-striped
    - if @users.empty?
      %tr
        %td{:colspan => 7} Empty submissions.
    - else
      = render partial: "users_list", collection: @users, as: :user